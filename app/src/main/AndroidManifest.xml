<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.androidapplication">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_perso"
        android:label="@string/NAME"
        android:roundIcon="@mipmap/ic_launcher_perso_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AndroidApplication">

        <activity android:name="Controler.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="Controler.AddUser" />
        <activity android:name="Controler.LoginUser"/>
    </application>

</manifest>